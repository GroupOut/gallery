{
  "name": "descriptions",
  "version": "1.0.0",
  "description": "merchant and deal information",
  "repository": {
    "type": "git",
    "url": "https://github.com/GroupOut/descriptions.git"
  },
  "author": "Christian Noh",
  "license": "ISC",
  "engines": {
    "node": ">=6.13.0"
  },
  "main": "bundle.js",
  "scripts": {
    "lint": "eslint '**/*.{js,jsx}' --quiet",
    "format": "prettier --write '**/*.{js,jsx}'",
    "build": "webpack -d",
    "test": "jest",
    "start": "node server.js"
  },
  "dependencies": {
    "aws-sdk": "^2.280.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.3",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.1",
    "jquery": "^3.3.1",
    "mysql": "^2.16.0",
    "nodemon": "^1.18.3",
    "path": "^0.12.7",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-imgpro": "^1.3.7",
    "sinon": "^6.1.4",
    "style-loader": "^0.21.0",
    "styled-components": "^3.3.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "identity-obj-proxy": "^3.0.0",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "<rootDir>/client/src/components/Navbar.jsx"
    ]
  }
}
